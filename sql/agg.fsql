SET PARALLEL_DEGREE = 2;

CATALOG TABLE LINEITEM( "TYPE" = "PARQUET", PATH="/Users/adarshrp/Projects/tpch-data/sf10/lineitem.parquet", PARTITIONS = 2);
DESCRIBE TABLE LINEITEM;

SELECT SUM(L_QUANTITY) AS SUM_QTY, AVG(L_QUANTITY) AS AVG_QTY, COUNT(L_QUANTITY) AS COUNT_QTY, MAX(L_RETURNFLAG), L_RETURNFLAG, L_LINESTATUS
FROM LINEITEM
GROUP BY L_RETURNFLAG, L_LINESTATUS;

