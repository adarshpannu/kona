CATALOG TABLE emp ( "TYPE" = "CSV", "PATH" = "/Users/adarshrp/Projects/flare/data/emp.csv", "HEADER" = "YES", "SEPARATOR" = "," );

DESCRIBE TABLE emp;

select sum(age + 10)*99 / count(age + 50), dept_id + 55, avg(distinct age), dept_id + 55
from emp E
where 
    age > 50 
and dept_id < 99
group by dept_id + 55
having sum(age) > 100 and dept_id + 55 > 10
;

