CATALOG TABLE emp ( "TYPE" = "CSV", "PATH" = "/Users/adarshrp/Projects/flare/data/emp.csv", "HEADER" = "YES", "SEPARATOR" = "," );

DESCRIBE TABLE emp;

set PARSE_ONLY = "true";

select sum(age + 10)*99 / count(age + 50), emp_dept_id + 55, avg(distinct age), emp_dept_id + 55
from emp E
where 
    age > 50 
and emp_dept_id < 99
group by emp_dept_id + 55
having sum(age) > 100 and emp_dept_id + 55 > 10
;

