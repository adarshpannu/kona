CATALOG TABLE emp ( "TYPE" = "CSV", "PATH" = "/Users/adarshrp/Projects/flare/data/emp.csv", PARTITIONS = 3);
CATALOG TABLE dept ( "TYPE" = "CSV", "PATH" = "/Users/adarshrp/Projects/flare/data/dept.csv", PARTITIONS = 3);

DESCRIBE TABLE emp;
DESCRIBE TABLE dept;

set PARALLEL_DEGREE = 4;

// All engineers aged 35+ 
SELECT EMP.name, DEPT.DEPT_ID 
from EMP, DEPT
where EMP.EMP_DEPT_ID = DEPT.DEPT_ID
AND EMP.age > 35
AND DEPT.NAME = "Engineering"
;

