CATALOG TABLE emp ( "TYPE" = "CSV", "PATH" = "/Users/adarshrp/Projects/yard/data/emp.csv", PARTITIONS = 1);
CATALOG TABLE dept ( "TYPE" = "CSV", "PATH" = "/Users/adarshrp/Projects/yard/data/dept.csv", PARTITIONS = 1);

DESCRIBE TABLE emp;
DESCRIBE TABLE dept;

set PARALLEL_DEGREE = 1;

// All engineers aged 35+ 
SELECT EMP.age, EMP.name, DEPT.DEPT_ID, EMP.AGE + DEPT.DEPT_ID
from EMP, DEPT
where EMP.EMP_DEPT_ID = DEPT.DEPT_ID
AND EMP.age > 35
AND DEPT.NAME = "Engineering"
;

