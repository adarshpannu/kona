{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "run main",
			"type": "cargo",
			"command": "run",
			"args": [
				"sql/tpch-q1.fsql",
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": false
			}
		},
		{
			"label": "run_unit_tests",
			"type": "cargo",
			"command": "test",
			"args": [
				"run_unit_tests",
				"--",
				"--nocapture"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": false
			}
		},
	]
}
